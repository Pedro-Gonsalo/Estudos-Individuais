unit uForWards;

interface

type
  {Forward declarações}

  // Declaração prévia da classe para evitar que o compilador de erro
  TCliente = class;

  TClienteEndereco = class
    Logadouro: String;
    Numero: String;
    Complemento: String;
    Cliente: TCliente;
  end;

  TCliente = class
    Nome: String;
    Telefone: String;
    Endereco: TClienteEndereco;
  end;

implementation

end.
